{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Serilog": {
    "MinumumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "Elasticsearch": {
    "BaseUri": "http://localhost:9200/",
    "EmployeesIndexName": "employees",
    "AppName": "example-elasticsearch-api"
  }  
}
